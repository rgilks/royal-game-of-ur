    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "aws-amplify": "^6.5.0",
    "howler": "^2.2.4",
    "react": "^18.3.1",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-ios-pwa-prompt": "^2.0.6",
    "use-sync-external-store": "^1.2.2",
